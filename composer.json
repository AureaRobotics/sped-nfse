{
    "name": "nfephp-org/sped-nfse",
    "type": "library",
    "description": "Classes para geração e comunicação de NFSe.",
    "keywords": [
        "nfse",
        "sped",
        "serviços"
    ],
    "homepage": "https://github.com/nfephp-org/sped-nfse",
    "license": [
        "GPL-3.0+",
        "LGPL-3.0+"
    ],
    "authors": [
        {
          "name": "Thiago Colares",
          "email": "thicolares@gmail.com>",
          "homepage": "http://www.nfephp.org",
          "role": "Developer"
        },
        {
            "name": "Comunidade NFePHP",
            "homepage": "https://github.com/nfephp-org/nfephp/graphs/contributors"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "ext-dom": "*",
        "ext-xml": "*",
        "ext-libxml": "*",
        "ext-curl": "*",
        "ext-soap": "*",
        "ext-openssl": "*",
        "ext-iconv": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.7.7"
    },
    "autoload": {
        "psr-4": {
          "Sped\\NFSe\\": "src/" 
        } 
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev",
            "dev-develop": "2.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload-dev": {
        "psr-4": {
            "SpedTest\\NFSe\\": "test"
        }
    }
}